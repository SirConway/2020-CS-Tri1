
void setup(){
  size(800,800);
  background(#7EFFFC);
  ellipse(400,400,100,100);
  fill(255,255,0);
}

void draw(){
  if(mouseX>350 && mouseY>350 && mouseX<450 && mouseY<450){
      background(255,255,0);
      ellipse(400,400,100,100);
      fill(#7EFFFC);
    }
    else
    background(#7EFFFC);
    ellipse(400,400,100,100);
    fill(255,255,0);
}
