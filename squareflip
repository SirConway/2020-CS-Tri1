
void setup(){
  size(800,800);
  background(#7EFFFC);
  rect(350,350,100,100);
  fill(255,255,0);
}

void draw(){
  if(mouseX>350 && mouseY>350){
    background(#7EFFFC);
    rect(350,350,100,100);
    fill(255,255,0);
    if(mouseX<450 && mouseY<450){
      background(255,255,0);
      rect(350,350,100,100);
      fill(#7EFFFC);
    }
    else
    background(#7EFFFC);
    rect(350,350,100,100);
    fill(255,255,0);
}

}
